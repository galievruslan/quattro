<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @customer.name %>
</p>
<table class="table table-striped table-hover">
  <thead>
    <tr>
        <th>Contact Type</th>
        <th>Contact</th>
    </tr>
  </thead>
  <tbody>
    <% @customer.contacts.each do |contact| %>  
    <tr>
      <td><%= contact.type %></td>
      <td><%= contact.value %></td>
    </tr>
    <%end%>
  </tbody>
</table>

<%= link_to 'Edit', edit_administration_customer_path(@customer) %> |
<%= link_to 'Back', administration_customers_path %>
