<section id="main" class="round">
  <h1>Contacts</h1>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Contact type</th>
        <th>Contact</th> 
      </tr>
    <thead>
    <tbody>
    <% @contacts.each do |contact| %>  
      <tr>
        <td><%= contact.type %></td>
        <td><%= contact.value %></td>
      </tr>  
    <% end %>
    <tbody>
  </table>
</section>